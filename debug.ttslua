local debug = {}

function debug.zprint(var,title)
    if (type(var) == "table") then
        if title then
            print(logString(var,'\n-- '..title..' --'))
        else
            print(logString(var))
        end
    else
        if title then print('-- '..title..' --') end
        print(var)
    end
end

return debug
